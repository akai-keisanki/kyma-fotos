# Kyma Rythmiseis Package For LuxOS 1.0 ( Unfinished )
# Kyma's Settings And Configuration For LuxOS 1.0

class lux_pack:
  name="kyma-rythmiseis"
  version="1.0"
  aditionaldata={"datalist":[None]}
  def startscript():
    {}
  def pexcm(cmd, cliv):
    splitcmd=cmd.split(" ")
    r=1
    if splitcmd[0] == "k-rythmiseis" or splitcmd[0] == "kyma-rythmiseis" or splitcmd[0] == "kyma-rythmiseis-kosmos":
      try:
        if self.splitcmd[1] == "#version":
          print("Κύμα Ρυθμίσεις 1.0 - \" Κόσμος\"")
      except:
          print(cmd+" >> kyma-rythmiseis failed.")
    else:
      r=0
    return r
  class packvars:
    kyma_name="Κύμα"
    kyma_codename="Κόσμος"
